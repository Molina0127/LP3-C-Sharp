using System;

namespace Quest_06_Ex_04
{
	
	public class Program
	{
		public static void Main()
		{
			//Alimentando uma Matriz//
			
			string[,] dados_alunos = new string[3, 4];
			
			int i = 0; 
			int j = 0;
			
			for(i = 0; i < 3; i++){
				Console.WriteLine("Lendo informações do " + (i + 1) + "° aluno:");
				for(j = 0; j < 4; j++)
				{
					Console.Write("Digite dados[" + i + ", " + j + "]: ");
					dados_alunos[i, j] = Console.ReadLine();
				}
			}
		}
	}
}
